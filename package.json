  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-/node-doix-devextreme.git"
  },
  "keywords": [
    "doix",
    "DevExtreme"
  ],
  "author": "Dmitry Ovsyanko",
  "peerDependencies": {
    "doix-db": "^1.0.39"
  },
  "devDependencies": {
    "jest": "^29.6.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/do-/node-doix-devextreme/issues"
  },
  "homepage": "https://github.com/do-/node-doix-devextreme#readme"
