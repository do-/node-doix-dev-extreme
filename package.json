{
  "name": "doix-devextreme",
  "version": "1.0.1",
  "description": "doix binding for DevExtreme framefork",
  "main": "index.js",
  "files": ["/lib", "/badges"],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
	"collectCoverageFrom": [
		"lib/**/*.js"
	],
	"coverageReporters": ["clover", "json", "lcov", "text", "json-summary"],
